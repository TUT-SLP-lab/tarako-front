{
  "name": "tarako-front",
  "version": "0.1.0",
  "engines": {
    "node": ">=18.16.0"
  },
  "private": true,
  "scripts": {
    "dev": "next dev",
    "mock": "prism mock ./openapi.yaml",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "next lint --fix && prettier --write .",
    "prepare": "husky install",
    "generate-typescript-axios": "openapi-generator-cli generate -g typescript-axios -i ./openapi.yaml -o ./src/generated/typescript-axios",
    "download-open-api-yaml": "curl -o openapi.yaml https://tarako-aws-sam.s3.ap-northeast-1.amazonaws.com/dev/openapi.yaml"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@mantine/core": "^6.0.19",
    "@mantine/hooks": "^6.0.19",
    "@mantine/next": "^6.0.19",
    "@tabler/icons-react": "^2.32.0",
    "@types/node": "20.5.7",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/react-speech-recognition": "^3.9.2",
    "autoprefixer": "10.4.15",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.9",
    "destyle.css": "^4.0.0",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "lodash": "^4.17.21",
    "next": "13.4.19",
    "postcss": "8.4.28",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.7",
    "react-speech-recognition": "^3.10.0",
    "recharts": "^2.8.0",
    "swr": "^2.2.2",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "use-local-storage": "^3.0.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@stoplight/prism-cli": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3"
  }
